{
  "name": "@medical-imaging/viewer",
  "version": "1.0.0",
  "description": "Medical imaging viewer application built with React 18 and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest --run test/performance",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "benchmark": "node ../../scripts/performance-benchmark.js"
  },
  "dependencies": {
    "@cornerstonejs/core": "^1.86.1",
    "@cornerstonejs/dicom-image-loader": "^1.86.0",
    "@cornerstonejs/streaming-image-volume-loader": "^1.86.1",
    "@cornerstonejs/tools": "^1.86.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@kitware/vtk.js": "^34.13.1",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.5",
    "@mui/x-date-pickers": "^6.10.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "cornerstone-wado-image-loader": "^4.13.2",
    "date-fns": "^2.30.0",
    "dicom-parser": "^1.8.21",
    "jspdf": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.1",
    "c8": "^8.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.1"
  }
}
